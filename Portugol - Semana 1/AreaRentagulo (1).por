programa {
  funcao inicio() {
    real base, altura, areaRetangulo
    escreva("Digite o valor para a base do retângulo: ")
    leia(base)

    escreva("Digite o valor para a altura do retângulo: ")
    leia(altura)

    areaRetangulo = base * altura
    escreva("A área do rentagulo é: ", areaRetangulo)

  }
}
